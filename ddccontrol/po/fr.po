msgid "Control"
msgstr "Contrôle"

#: src/ddccontrol/main.c:104
#, c-format
msgid ""
"Usage:\n"
"%s [-v] [-c] [-d] [-f] [-s] [-r ctrl] [-w value] [-p | dev]\n"
"\tdev: device, e.g. /dev/i2c-0\n"
"\t-p : probe I2C devices to find monitor busses.\n"
"\t-c : query capability\n"
"\t-d : query ctrls 0 - 255\n"
"\t-r : query ctrl\n"
"\t-w : value to write to ctrl\n"
"\t-f : force (avoid validity checks)\n"
"\t-s : save settings\n"
"\t-v : verbosity (specify more to increase)\n"
msgstr ""
"Usage:\n"
"%s [-v] [-c] [-d] [-f] [-s] [-r ctrl] [-w value] [-p | dev]\n"
"\tdev: périphérique, par exemple /dev/i2c-0\n"
"\t-p : interroge les périphériques I2C pour trouver un moniteur.\n"
"\t-c : query capability\n"
"\t-d : query ctrls 0 - 255\n"
"\t-r : query ctrl\n"
"\t-w : value to write to ctrl\n"
"\t-f : force (avoid validity checks)\n"
"\t-s : save settings\n"
"\t-v : verbosity (specify more to increase)\n"

